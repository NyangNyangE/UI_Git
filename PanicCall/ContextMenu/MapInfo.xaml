<Window x:Class="PanicCall.MapInfo"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:local="clr-namespace:PanicCall"
	mc:Ignorable="d"
	x:Name="Window"
	Title="MapInfo" ShowInTaskbar="False" Background="{x:Null}" Width="310" Height="120" WindowStyle="None" ResizeMode="NoResize" Topmost="True" AllowsTransparency="True">

    <Grid x:Name="LayoutRoot" Height="Auto" Width="Auto">
        <Rectangle Height="100" Width="300" Fill="White"  RadiusX="20" RadiusY="20" Margin="5,15.987,5,4.013"/>
        <TextBlock Text="맵 정보를 입력 하세요"  Width="200" TextAlignment="Center" FontSize="13.333" FontWeight="Bold" Margin="50,23.505,50,0" VerticalAlignment="Top" Height="24.977" />
		<TextBlock Text="이름"  Width="50" Margin="19,0,0,30.994" HorizontalAlignment="Left" d:LayoutOverrides="Width" VerticalAlignment="Bottom" Height="20" />
		<TextBox  x:Name="name" Margin="71.5,0,78.5,31.994" VerticalAlignment="Bottom" Height="20"  /> 
		<Button x:Name="comfirm" Width="30" Content="확인" Margin="0,59.006,23,40.994" HorizontalAlignment="Right" Click="comfirm_Click" />
		<Button x:Name="cancel" Height="20" Width="30" Content="취소" Margin="0,0,23,16.994" VerticalAlignment="Bottom" d:LayoutOverrides="Width" HorizontalAlignment="Right" Click="cancel_Click" />
	
		
	</Grid>
</Window>
